<?xml version="1.0" encoding="utf-8" ?>
<grid>
  <actions>
    <action name="edit" caption="$General.Edit$" fonticonclass="icon-edit" fonticonstyle="allow" />
    <action name="#delete" caption="$General.Delete$" fonticonclass="icon-bin" fonticonstyle="critical" confirmation="$General.ConfirmDelete$" />
  </actions>
  <columns>
    <column source="HashDisplayName" caption="Name" wrap="false" localize="true">
      <filter type="text" size="200" />
    </column>
    <column source="HashSecureUrl" caption="Link" externalsourcename="#getresthashtrackerlink" />
    <column source="HashUrl" caption="Response Type" width="100%" wrap="false" localize="true" externalsourcename="#getresthashtrackertype"/>
    
  </columns>
  <options>
    <key name="DisplayFilter" value="true" />
    <key name="FilterLimit" value="5" />
  </options>
</grid>